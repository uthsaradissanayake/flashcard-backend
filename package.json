{
  "name": "flashcard-backend",
  "version": "1.0.0",
  "description": "Flashcard app backend with FSRS spaced repetition",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "postinstall": "npm run build",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "ts-fsrs": "^3.4.0",
    "pg": "^8.11.3",
    "knex": "^2.5.1",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.17",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/cors": "^2.8.14",
    "@types/pg": "^8.10.3",
    "@types/compression": "^1.7.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1"
  },
  "keywords": ["flashcard", "spaced-repetition", "fsrs", "education"],
  "author": "Your Name",
  "license": "MIT"
}